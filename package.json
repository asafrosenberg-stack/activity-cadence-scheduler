{
  "name": "activity-cadence",
  "version": "1.0.0",
  "description": "Automated weekly activity scheduler with Google Calendar, Notion, and Gmail integration",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test-run": "ts-node src/index.ts --test",
    "run-now": "ts-node src/index.ts --run-now",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "postinstall": "npm run build"
  },
  "keywords": ["scheduler", "automation", "calendar", "notion", "gmail"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "googleapis": "^126.0.1",
    "@notionhq/client": "^2.2.13",
    "dotenv": "^16.3.1",
    "nodemailer": "^6.9.7",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "node-cron": "^3.0.2",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@types/node": "^20.8.7",
    "@types/nodemailer": "^6.4.14",
    "@types/node-cron": "^3.0.8",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "eslint": "^8.52.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.6",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1"
  }
}